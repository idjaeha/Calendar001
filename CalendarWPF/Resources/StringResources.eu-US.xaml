<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CalendarWPF.Resources"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Greeting">Hello</system:String>
    <system:String x:Key="ProgramExit">Program Exit</system:String>
    <system:String x:Key="ProgramHiding">Program Hide</system:String>
    <system:String x:Key="Dragging">Change position and size</system:String>
    <system:String x:Key="noDragging">Fix position and size</system:String>
    <system:String x:Key="ChangeKorean">Change Korean</system:String>
    <system:String x:Key="ChangeEnglish">Change English</system:String>
    <system:String x:Key="Setting">Setting</system:String>
</ResourceDictionary>